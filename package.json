{
  "name": "multi-export",
  "version": "0.2.6",
  "description": "Multi export for import into an A5 DB",
  "main": "main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -w --ia32",
    "rmad": "rm -f -r %APPDATA%/multi-export",
    "test": "jest",
    "lint": "standard",
    "scss": "sass .\\multi-export.scss .\\multi-export.css"
  },
  "repository": "https://github.com/sergey-untilov/multi-export",
  "keywords": [
    "Electron",
    "export",
    "A5"
  ],
  "author": "UncleZeppelin",
  "license": "MIT",
  "build": {
    "appId": "multi-export",
    "win": {
      "target": "portable",
      "icon": "build/multi-export.png"
    },
    "directories": {
      "buildResources": "build"
    },
    "extraFiles": [
      {
        "from": "assets",
        "to": "assets"
      }
    ]
  },
  "devDependencies": {
    "devtron": "^1.4.0",
    "dotenv": "^8.2.0",
    "electron": "^7.1.8",
    "electron-builder": "^22.2.0",
    "jest": "^24.9.0",
    "standard": "^14.3.1"
  },
  "dependencies": {
    "archiver": "^3.1.1",
    "electron-reload": "^1.5.0",
    "electron-store": "^5.1.0",
    "iconv": "^2.3.5",
    "iconv-lite": "^0.5.0",
    "mssql": "^6.0.1",
    "node-sass": "^4.13.0",
    "sass": "^1.24.0",
    "spectre.css": "^0.5.8",
    "yadbf": "^3.2.3"
  }
}
